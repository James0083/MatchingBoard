<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.multi.mapper.EvalCafeMapper">
    <update id="updateManners" parameterType="User">
        update user set
        set manner=manner+#{avgRatings}<!-- 기존 별점에 새로 유입된 평균별점 계산하는 코드 필요 -->
        <where>
			userid=#{userid}<!-- list형태로 참가자 전원을 받아오는데 user별로 입력받을 방법 필요 -->
        </where>
    </update>
</mapper>