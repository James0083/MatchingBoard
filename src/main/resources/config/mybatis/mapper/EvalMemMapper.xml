<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.multi.mapper.EvalMemMapper">
    <select id="listUser" resultType="UserVO">
        SELECT * FROM user;
    </select>

    <update id="updateManners" parameterType="MemberEvalVO">
        UPDATE user
        SET manner = manner + #{eval}
        WHERE userid = #{userid}
    </update>
</mapper>
